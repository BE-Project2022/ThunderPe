{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_Yel.png",
    "splash": {
      "image": "./assets/images/Logo_white.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFC100"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo_Yel.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/Logo_Yel.png"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffc100"
    }
  }
}